{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/strict": "off",
    "button-type": "off",
    "no-inline-styles": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off",
        "duplicate-id-active": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "rotate",
          "width: fit-content"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 113",
    "not firefox <= 113",
    "not ios_saf <= 16.5",
    "not safari <= 16.5"
  ]
}